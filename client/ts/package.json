    "@synnaxlabs/vite-plugin": "workspace:*",
    "@types/node": "^18.11.18",
    "eslint-config-synnaxlabs": "workspace:*",
    "vite": "^4.0.4",
    "vitest": "^0.26.3"
  },
  "files": [
    "dist"
  ],
  "main": "dist/client.cjs.js",
  "module": "dist/client.es.js",
  "types": "dist/index.d.ts"
