{
  "name": "@synnaxlabs/client",
  "private": false,
  "version": "0.2.1",
  "type": "module",
  "description": "The Client Library for Synnax",
  "repository": "https://github.com/synnaxlabs/synnax/tree/main/client/ts",
  "license": "MIT",
  "keywords": [
