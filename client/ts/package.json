{
  "name": "@synnaxlabs/client",
  "private": false,
  "version": "0.2.1",
  "description": "The Client Library for Synnax",
  "repository": "https://github.com/synnaxlabs/synnax/tree/main/client/ts",
  "license": "MIT",
  "keywords": [
    "synnax",
    "grpc",
    "transport",
    "client",
    "data acquisition",
    "big data",
    "telemetry",
    "control systems"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.module.json",
    "watch": "tsc -p tsconfig.json -w",
    "lint": "eslint \"src/**/*.ts*\"",
    "fix": "eslint \"src/**/*.ts*\" --fix",
    "test": "nyc --silent ava",
    "cov": "nyc --silent ava -s && nyc report --reporter=lcov && nyc report --reporter-html"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@synnaxlabs/freighter": "*",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "ava": "^3.12.1",
    "codecov": "^3.5.0",
    "cspell": "^4.1.0",
    "eslint": "^7.8.0",
    "gh-pages": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "open-cli": "^6.0.1",
    "prettier": "^2.1.1",
    "standard-version": "^9.0.0",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.0",
    "typescript": "^4.0.2",
    "eslint-config-synnaxlabs": "*"
  },
  "files": [
    "dist/main",
    "dist/module",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "ava": {
    "failFast": true,
    "timeout": "60s",
    "typescript": {
      "rewritePaths": {
        "src/": "dist/main/"
      }
    },
    "files": [
      "!dist/module/**"
    ]
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "main": "dist/main/index.js",
  "typings": "dist/main/index.d.ts",
  "module": "dist/module/index.js"
}
