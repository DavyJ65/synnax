{
  "name": "@synnaxlabs/client",
  "private": false,
  "version": "0.2.1",
  "description": "The Client Library for Synnax",
  "repository": "https://github.com/synnaxlabs/synnax/tree/main/client/ts",
  "license": "MIT",
  "keywords": [
    "synnax",
    "grpc",
    "transport",
    "client",
    "data acquisition",
    "big data",
    "telemetry",
    "control systems"
  ],
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "cov": "vitest --coverage",
    "lint": "eslint \"src/**/*.ts*\"",
    "fix": "eslint \"src/**/*.ts*\" --fix"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@synnaxlabs/freighter": "*",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@synnaxlabs/tsconfig": "*",
    "eslint-config-synnaxlabs": "*",
    "vite": "^4.0.1",
    "vite-plugin-dts": "^1.7.1",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "^0.25.8"
  },
  "files": [
    "dist/main",
    "dist/module",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "ava": {
    "failFast": true,
    "timeout": "60s",
    "typescript": {
      "rewritePaths": {
        "src/": "dist/main/"
      }
    },
    "files": [
      "!dist/module/**"
    ]
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "main": "dist/client.cjs.js",
  "typings": "dist/index.d.ts",
  "module": "dist/client.es.js"
}
