{
  "name": "@synnaxlabs/drift",
  "private": false,
  "version": "0.1.0",
  "description": "State synchronization and Redux state synchronization for Tauri Apps",
  "repository": "https://github.com/synnaxlabs/synnax/tree/main/drift",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "synnax",
    "tauri",
    "redux",
    "react",
    "electron",
    "application level state"
  ],
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "watch": "tsc --noEmit && vite build --watch",
    "test": "vitest",
    "cov": "vitest --coverage",
    "lint": "eslint \"src/**/*.ts*\"",
    "fix": "eslint \"src/**/*.ts*\" --fix"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.1",
    "@tauri-apps/api": "^1.1.0",
    "proxy-memoize": "^1.2.0",
    "react": "^18.2.0",
    "react-redux": "^8.0.4"
  },
  "devDependencies": {
    "@synnaxlabs/tsconfig": "workspace:*",
    "@synnaxlabs/vite-plugin": "workspace:*",
    "@types/react": "^18.0.26",
    "@vitejs/plugin-react": "^3.0.1",
    "eslint-config-synnaxlabs": "workspace:*",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vite-plugin-lib": "^1.0.1",
    "vitest": "^0.26.2"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/drift.cjs.js",
  "module": "./dist/drift.es.js",
  "types": "./dist/index.d.ts"
}
