{
  "name": "@synnaxlabs/freighter",
  "version": "0.2.0",
  "description": "a modular transport abstraction",
  "main": "dist/main/index.js",
  "typings": "dist/main/index.d.ts",
  "module": "dist/module/index.js",
  "repository": "https://github.com/synnaxlabs/synnax",
  "license": "MIT",
  "keywords": [
    "synnax",
    "grpc",
    "transport",
    "client",
    "data acquisition",
    "big data",
    "telemetry",
    "control systems"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.module.json",
    "test": "nyc --silent ava",
    "cov": "nyc --silent ava && nyc report --reporter=lcov && nyc report --reporter=html",
    "lint": "TIMING=1 eslint \"src/**/*.ts*\""
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "js-convert-case": "^4.2.0",
    "msgpackr": "^1.7.0",
    "ws": "^8.8.1",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "ava": "^3.12.1",
    "codecov": "^3.5.0",
    "cspell": "^4.1.0",
    "eslint": "^7.8.0",
    "eslint-config-synnaxlabs": "*",
    "gh-pages": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "open-cli": "^6.0.1",
    "prettier": "^2.7.1",
    "standard-version": "^9.0.0",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.0",
    "typescript": "^4.0.2"
  },
  "files": [
    "dist/main",
    "dist/module",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "ava": {
    "failFast": true,
    "timeout": "60s",
    "typescript": {
      "rewritePaths": {
        "src/": "dist/main/"
      }
    },
    "files": [
      "!dist/module/**"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
