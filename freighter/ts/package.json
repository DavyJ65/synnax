{
  "name": "@synnaxlabs/freighter",
  "private": false,
  "version": "0.2.0",
  "type": "module",
  "description": "a modular transport abstraction",
  "repository": "https://github.com/synnaxlabs/synnax/tree/main/freighter/ts",
  "license": "MIT",
  "keywords": [
    "synnax",
    "grpc",
    "transport",
    "data acquisition",
    "big data",
    "telemetry",
    "control systems"
  ],
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "watch": "tsc --noEmit && vite build --watch",
    "test": "vitest",
    "cov": "vitest --coverage",
    "lint": "eslint \"src/**/*.ts*\"",
    "fix": "eslint \"src/**/*.ts*\" --fix"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "js-convert-case": "^4.2.0",
    "msgpackr": "^1.7.0",
    "ws": "^8.8.1",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@synnaxlabs/tsconfig": "*",
    "eslint-config-synnaxlabs": "*",
    "typescript": "^4.0.2",
    "vite": "^4.0.1",
    "vite-plugin-dts": "^1.7.1",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "^0.25.8"
  },
  "exports": {
    ".": {
      "import": "./dist/freighter.es.js",
      "require": "./dist/freighter.cjs.js",
      "types": "./dist/freighter.es.d.ts"
    }
  }
}
